<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.dcmd.service.demand.mapper.InstitutionMapper">

    <select id="queryInstitutionNoAndName" resultType="com.dcmd.service.demand.entity.Institution">

        SELECT
        InstNo as "instNo",
        InstName as "instName"
        FROM
        t_institution

    </select>
</mapper>
